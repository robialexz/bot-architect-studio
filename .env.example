# App Configuration
VITE_APP_NAME=AI Workflow Studio
VITE_APP_VERSION=1.0.0
VITE_APP_URL=https://your-domain.vercel.app
VITE_APP_ENV=production

# Supabase Configuration (Required)
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Example:
# VITE_SUPABASE_URL=https://your-project-ref.supabase.co
# VITE_SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...

# AI Services Configuration (Optional - will use demo mode if not provided)
# OpenAI API Key (for GPT-4, GPT-3.5, DALL-E)
# Get your key from: https://platform.openai.com/api-keys
VITE_OPENAI_API_KEY=demo-key

# Anthropic API Key (for Claude)
# Get your key from: https://console.anthropic.com/
VITE_ANTHROPIC_API_KEY=demo-key

# Hugging Face API Key (for various AI models)
# Get your key from: https://huggingface.co/settings/tokens
VITE_HUGGINGFACE_API_KEY=demo-key

# Stability AI API Key (for Stable Diffusion)
# Get your key from: https://platform.stability.ai/account/keys
VITE_STABILITY_API_KEY=demo-key

# Cohere API Key (for language models)
# Get your key from: https://dashboard.cohere.ai/api-keys
VITE_COHERE_API_KEY=demo-key

# Workflow Execution Configuration
VITE_ENABLE_REAL_AI_EXECUTION=true
VITE_WORKFLOW_EXECUTION_TIMEOUT=300000
VITE_MAX_CONCURRENT_EXECUTIONS=10

# Backend API Configuration
VITE_API_BASE_URL=http://localhost:8000
VITE_API_TIMEOUT=30000
VITE_BACKEND_ENABLED=true

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=false
VITE_ENABLE_PERFORMANCE_MONITORING=false

# Session Management
VITE_ENABLE_SESSION_PERSISTENCE=true

# Logging Configuration
# LOG_LEVEL options: debug, info, warn, error
# In production, use error to disable all debug logging
# In development, use debug for full logging
VITE_LOG_LEVEL=debug
VITE_LOG_ENDPOINT=

# Security Configuration
VITE_CSP_NONCE=
VITE_ALLOWED_ORIGINS=

# Third-party Services
VITE_SENTRY_DSN=
VITE_GOOGLE_ANALYTICS_ID=
VITE_HOTJAR_ID=

# Development Configuration
VITE_MOCK_API=false
VITE_DEBUG_MODE=false
